ce":"Details.js","original":{"line":47,"column":12}},{"generated":{"line":112,"column":218},"source":"Details.js","original":{"line":46,"column":8}},{"generated":{"line":113,"column":0},"source":"Details.js","original":{"line":56,"column":5}},{"name":"React","generated":{"line":117,"column":2},"source":"Details.js","original":{"line":10,"column":22}},{"name":"Component","generated":{"line":117,"column":17},"source":"Details.js","original":{"line":10,"column":28}},{"name":"Details","generated":{"line":119,"column":15},"source":"Details.js","original":{"line":59,"column":15}}],"sources":{"Details.js":"import React from \"react\";\nimport pf from \"petfinder-client\";\nimport Carousel from \"./Carousel\";\n\nconst petfinder = pf({\n    key: process.env.API_KEY,\n    secret: process.env.API_SECRET\n});\n\nclass Details extends React.Component {\n    state = {\n        loading: true \n    };\n    componentDidMount() {\n        petfinder.pet\n            .get({\n                output: \"full\",\n                id: this.props.id\n            })\n            .then(data => {\n                let breed;\n                if(Array.isArray(data.petfinder.pet.breeds.breed)) {\n                    breed = data.petfinder.pet.breeds.breed.join(\", \");\n                } else {\n                    breed = data.petfinder.pet.breeds.breed;\n                }\n                this.setState({\n                    name: data.petfinder.pet.name,\n                    animal: data.petfinder.pet.animal,\n                    location: `${data.petfinder.pet.contact.city}, ${data.petfinder.pet.contact.state}`,\n                    description: data.petfinder.pet.description,\n                    media: data.petfinder.pet.media,\n                    breed,\n                    loading: false\n                });\n            })\n            .catch(err => this.setState({ error: err }));\n    }\n    render() {\n        if (this.state.loading) {\n            return <h1>loading ... </h1>;\n        }\n\n        const { animal, breed, location, description } = this.state;\n\n        return (\n            <div className=\"details\">\n                <div>\n                    <Carousel media={media} />\n                    <h1>{name}</h1>\n                    <h2>{`${animal} - ${breed} - ${location}`}</h2>\n                    <p>{description}</p>\n                </div>\n            </div>\n        );\n    }\n}\n\nexport default Details;"},"lineCount":null}},"error":null,"hash":"48c41bb6cf4a90b24a8216548cbbe5d9","cacheData":{"env":{}}}